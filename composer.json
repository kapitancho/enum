{
    "name": "walnut/enum",
	"keywords": [
		"walnut",
		"enum"
	],
    "version": "0.1",
    "description": "Minimalistic PHP 8 Enum Implementation",
    "license": "GNU GPL",
    "config" : {
        "vendor-dir" : "vendor"
    },
    "authors": [
        {
            "name": "Marian Kostadinov"
        }
    ],
    "require": {
        "php": ">=8.0"
	},
    "require-dev" : {
        "phpunit/phpunit": "^9.4.2",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Walnut\\Lib\\Enum\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Walnut\\Lib\\Enum\\Test\\": "tests/"
        }
    }
}